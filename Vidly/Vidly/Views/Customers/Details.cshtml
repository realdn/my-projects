

@model Vidly.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@if (Model == null)
{
    <h2>
        Sorry Not Found !!
    </h2>
}
else
{
    <h2>@Model.Name</h2>
    <ul>

        <li>@Model.MembershipType.Name</li>
        @if (Model.DateOfBirth!=null)
        {
            <li>@Model.DateOfBirth.Value.ToShortDateString()</li>

        }
            </ul>
}
    

